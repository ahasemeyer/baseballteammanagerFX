      <Button fx:id="lookupPlayerIDButton" layoutX="304.0" layoutY="94.0" mnemonicParsing="false" onAction="#handleLookupPlayerID" prefHeight="46.0" prefWidth="101.0" text="Lookup Player by ID" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="lookupPlayerNameButton" layoutX="304.0" layoutY="154.0" mnemonicParsing="false" onAction="#handleLookupPlayerName" prefHeight="46.0" prefWidth="101.0" text="Lookup Player by Name" textAlignment="CENTER" wrapText="true" />
