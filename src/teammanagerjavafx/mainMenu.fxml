      <Button fx:id="updatePlayerButton" layoutX="14.0" layoutY="338.0" mnemonicParsing="false" onAction="#handleUpdatePlayer" text="Update Player" />
      <Button fx:id="updateTeamButton" layoutX="361.0" layoutY="338.0" mnemonicParsing="false" onAction="#handleUpdateTeam" text="Update Team" />
      <Button fx:id="lookupPlayersButton" layoutX="329.0" layoutY="223.0" mnemonicParsing="false" onAction="#handleLookupPlayers" text="Lookup Players" />
   </children>
</AnchorPane>
