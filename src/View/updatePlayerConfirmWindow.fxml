   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Success!">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <Label fx:id="confirmMessageLabel" layoutX="14.0" layoutY="50.0" prefHeight="17.0" prefWidth="202.0" text="Your Player was successfully updated!" />
