<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="203.0" prefWidth="240.0" styleClass="anchorPane" stylesheets="@../CSS/mainmenu.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CreateTeamPageController">
   <children>
      <HBox layoutX="16.0" layoutY="14.0" prefHeight="55.0" prefWidth="208.0" styleClass="buttonPane" stylesheets="@../CSS/mainmenu.css">
         <children>
            <Label styleClass="titleText" stylesheets="@../CSS/mainmenu.css" text="Enter Team Name">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <VBox layoutX="30.0" layoutY="76.0" prefHeight="90.0" prefWidth="181.0" spacing="10.0" styleClass="buttonPane" stylesheets="@../CSS/mainmenu.css">
         <children>
            <TextField fx:id="teamName" promptText="Team Name" />
            <Button fx:id="createTeamButton" mnemonicParsing="false" onAction="#handleTeamCreate" styleClass="btn" stylesheets="@../CSS/mainmenu.css" text="Submit Team">
               <VBox.margin>
                  <Insets left="35.0" />
               </VBox.margin>
            </Button>
            <Label fx:id="errorMessageLabel" prefHeight="17.0" prefWidth="162.0" textAlignment="CENTER" textFill="RED" wrapText="true">
               <font>
                  <Font size="10.0" />
               </font></Label>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
