<AnchorPane id="AnchorPane" prefHeight="142.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.UpdateManagerPageController">
