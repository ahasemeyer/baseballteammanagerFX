      <Label layoutX="22.0" layoutY="26.0" text="Update Manager">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <ComboBox fx:id="chooseTeamCombo" layoutX="234.0" layoutY="32.0" onAction="#handleChooseTeam" prefWidth="150.0" promptText="Choose Team" />
      <ComboBox fx:id="chooseManagerCombo" layoutX="399.0" layoutY="32.0" onAction="#handleChooseManager" prefWidth="150.0" promptText="Choose Manager" />
      <Button fx:id="updateManagerButton" layoutX="384.0" layoutY="74.0" mnemonicParsing="false" onAction="#handleUpdateManager" text="Update Manager" />
      <TextField fx:id="winsText" layoutX="52.0" layoutY="74.0" prefHeight="25.0" prefWidth="62.0" text="0" />
      <TextField fx:id="tiesText" layoutX="278.0" layoutY="74.0" prefHeight="25.0" prefWidth="62.0" text="0" />
      <TextField fx:id="lossesText" layoutX="166.0" layoutY="74.0" prefHeight="25.0" prefWidth="62.0" text="0" />
      <Label fx:id="errorMessageLabel" layoutX="26.0" layoutY="109.0" prefHeight="17.0" prefWidth="494.0" textFill="RED" />
      <Label layoutX="12.0" layoutY="78.0" text="Wins:" />
      <Label layoutX="121.0" layoutY="78.0" prefHeight="17.0" prefWidth="42.0" text="Losses:" />
